<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:gviz="urn:import:com.google.gwt.visualization.client.visualizations">
	<ui:with field="mainRes" type="com.gmi.gwaswebapp.client.resources.MyResources" />
	<ui:style>
	.container {
		margin-top:10px;

	}
	</ui:style>
	<g:HTMLPanel>
		<div class="{style.container} {mainRes.style.box_shadow}">
			<div class="{mainRes.style.box_container}" >
				<g:HTMLPanel ui:field="container" />
			</div>
		</div>
		<div class="{style.container} {mainRes.style.box_shadow}">
			<div class="{mainRes.style.box_container}" >
				<g:DisclosurePanel animationEnabled="true" ui:field="disclosure_panel" width="100%">
					<g:header>Phenotype-Explorer</g:header>
				</g:DisclosurePanel>
			</div>
		</div>
			
	 <g:LayoutPanel ui:field="motionchart_container" height="100%" width="100%">
	 </g:LayoutPanel>
	</g:HTMLPanel>
</ui:UiBinder> 